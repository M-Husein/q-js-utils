{
  "name": "q-js-utils",
  "version": "1.2.7",
  "description": "A collection of JavaScript utilities.",
  "license": "MIT",
  "keywords": [
    "utilities",
    "utility",
    "utils",
    "simple",
    "advanced",
    "modern",
    "javascript",
    "js",
    "typescript",
    "ts",
    "fetch",
    "request",
    "number",
    "string",
    "object",
    "collection",
    "cached",
    "debounce",
    "throttle",
    "isEqual"
  ],
  "author": "Muhamad Husein <m.husein27@gmail.com>",
  "homepage": "https://github.com/M-Husein/q-js-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/M-Husein/q-js-utils.git"
  },
  "bugs": {
    "url": "https://github.com/M-Husein/q-js-utils/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./number": {
      "types": "./dist/number.d.ts",
      "import": "./dist/number.es.js",
      "require": "./dist/number.cjs.js"
    },
    "./cached": {
      "types": "./dist/cached.d.ts",
      "import": "./dist/cached.es.js",
      "require": "./dist/cached.cjs.js"
    },
    "./debounce": {
      "types": "./dist/debounce.d.ts",
      "import": "./dist/debounce.es.js",
      "require": "./dist/debounce.cjs.js"
    },
    "./debounceAdvanced": {
      "types": "./dist/debounceAdvanced.d.ts",
      "import": "./dist/debounceAdvanced.es.js",
      "require": "./dist/debounceAdvanced.cjs.js"
    },
    "./throttle": {
      "types": "./dist/throttle.d.ts",
      "import": "./dist/throttle.es.js",
      "require": "./dist/throttle.cjs.js"
    },
    "./throttleAdvanced": {
      "types": "./dist/throttleAdvanced.d.ts",
      "import": "./dist/throttleAdvanced.es.js",
      "require": "./dist/throttleAdvanced.cjs.js"
    },
    "./request": {
      "types": "./dist/request.d.ts",
      "import": "./dist/request.es.js",
      "require": "./dist/request.cjs.js"
    },
    "./nextId": {
      "types": "./dist/nextId.d.ts",
      "import": "./dist/nextId.es.js",
      "require": "./dist/nextId.cjs.js"
    },
    "./getInitials": {
      "types": "./dist/getInitials.d.ts",
      "import": "./dist/getInitials.es.js",
      "require": "./dist/getInitials.cjs.js"
    },
    "./darkOrLight": {
      "types": "./dist/darkOrLight.d.ts",
      "import": "./dist/darkOrLight.es.js",
      "require": "./dist/darkOrLight.cjs.js"
    },
    "./str2Hex": {
      "types": "./dist/str2Hex.d.ts",
      "import": "./dist/str2Hex.es.js",
      "require": "./dist/str2Hex.cjs.js"
    },
    "./obj2FormData": {
      "types": "./dist/obj2FormData.d.ts",
      "import": "./dist/obj2FormData.es.js",
      "require": "./dist/obj2FormData.cjs.js"
    },
    "./isEqual": {
      "types": "./dist/isEqual.d.ts",
      "import": "./dist/isEqual.es.js",
      "require": "./dist/isEqual.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "dev:example": "vite --config example/vite.config.ts",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "release": "release-it"
  },
  "devDependencies": {
    "@release-it/bumper": "^7.0.5",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/node": "^22.15.29",
    "@vitest/coverage-v8": "^3.1.4",
    "release-it": "^19.0.3",
    "terser": "^5.40.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.1.4"
  }
}
